		System.out.println(str + " ; words " + strArray.length + " ; char :" + charArray.length);
		
		Map charMap = new HashMap();
		Set setA = new HashSet();
		int start = 0;
		try{
			for(int i=0; i<charArray.length; i++){
				setA.add(charArray[i]);
			}			
		} catch (Exception e){
			System.out.println("i: " + i.toString());
		}

		
		
		
		

	}

